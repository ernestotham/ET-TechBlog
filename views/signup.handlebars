

<div class="container mb-4 w-50 bg-light border border-success rounded-3">
<br>
<form class="signup-form">

    <div class="mb-4">
        <label for="exampleInputName1" class="form-label">Name</label>
        <input type="name" class="form-control border border-success" id="SNameInput" aria-describedby="NameHelp">
        <div id="emailHelp" class="form-text"></div>
    </div>
    <div class="mb-4">
        <label for="exampleInputEmail1" class="form-label">Email address</label>
        <input type="email" class="form-control border border-success" id="SEmailInput" aria-describedby="emailHelp">
        <div id="emailHelp" class="form-text"></div>
    </div>
    <div class="mb-4">
        <label for="exampleInputPassword1" class="form-label">Password</label>
        <input type="password" class="form-control border border-success" id="SPasswordInput">
    </div>


    <div class="text-center">
        <button type="submit" class="btn btn-primary" id="SignedUpBtn">SignUp</button>
    </div>
</form>

<br>

</div>
<script src="./scripts/signedUp.js"></script>